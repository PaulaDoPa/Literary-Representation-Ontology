    <!-- http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#AnimationActor -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#AnimationActor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ActorFormat"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#LiveActionActor"/>
        <rdfs:comment>Animation means *ONLY* animation (if animation + liveaction -&gt; liveaction)</rdfs:comment>
    </owl:Class>
    


