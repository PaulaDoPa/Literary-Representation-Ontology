                    <rdf:Description rdf:about="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#Representation"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#hasFormat"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ActorFormat"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#hasFormat"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ScreenFormat"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


