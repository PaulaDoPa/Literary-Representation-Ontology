    <!-- http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ActionRepresentation -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ActionRepresentation">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#hasActorFormat"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#LiveActionActor"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ImageRepresentation"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ActorFormat -->

    <owl:Class rdf:about="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#ActorFormat">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/paula/ontologies/2019/11/untitled-ontology-48#RepresentationFormat"/>
    </owl:Class>
    


